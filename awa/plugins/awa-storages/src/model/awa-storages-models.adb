      end if;
      if Name = "mime_type" then
         return Util.Beans.Objects.To_Object (Impl.Mime_Type);
      end if;
      if Name = "file_size" then
         return Util.Beans.Objects.To_Object (Long_Long_Integer (Impl.File_Size));
