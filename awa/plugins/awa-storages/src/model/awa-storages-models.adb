                        Value => Integer (Storage_Type'Pos (Object.Storage)));
      Query.Save_Field (Name  => COL_4_3_NAME, --  uri
                        Value => Object.Uri);
      Query.Save_Field (Name  => COL_5_3_NAME, --  name
                        Value => Object.Name);
      Query.Save_Field (Name  => COL_6_3_NAME, --  mime_type
                        Value => Object.Mime_Type);
