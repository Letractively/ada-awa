with "awa";
project awa_workspaces is
   Version      := awa.Version;
   Name         := "awa-workspaces";
   for Library_Name use Name;
   for Library_Dir use "INSTALL/lib";
   for Library_Kind use "LIBRARY_TYPE";
   for Source_Dirs use ("INSTALL/share/ada/adainclude/" & Name);
   for Library_ALI_Dir use "INSTALL/lib/ada/adalib/" & Name & "/" & Project'Library_Kind;
   for Externally_Built use "true";
end awa_workspaces;
